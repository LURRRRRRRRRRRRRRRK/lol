local AkaliNotif = loadstring(game:HttpGet("https://raw.githubusercontent.com/LURRRRRRRRRRRRRRRK/lol/main/NOTIF"))();
local cocka = AkaliNotif.Notify;

  


--230, 35, 69



local colors = {
    SchemeColor = Color3.fromRGB(40,40,40),
    Background = Color3.fromRGB(13, 15, 12),
    Header = Color3.fromRGB(16, 18, 15),
    TextColor = Color3.fromRGB(255,255,255),
    ElementColor = Color3.fromRGB(20, 20, 20)
}
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Big Ballaz", colors)

    -- MAIN
    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("Main")

--[[MainSection:NewButton("Fall Bypass", "Bypasses fall damage", function()
game:GetService("ReplicatedStorage").Events.fall:Destroy()

end)]]

 
   -- PLAYER
   
   local Balls = Window:NewTab("Player ESP")
    local ESPSection = Balls:NewSection("ESP For Players")



ESPSection:NewToggle("Kiriot ESP", "ESP's Players using the KIRIOT ESP", function(state)
    if state then
        print("Toggle On")
    else
        print("Toggle Off")
    end
end)

local Binds = Window:NewTab("KeyBinds")
    local BindsSection = Binds:NewSection("Keybinds")
    
    
    
    
	local gkey = "B"
	local ukey = "M"
	local akey = "Q"

local Input = game:GetService('UserInputService')
	local ammo = false
	
	game:GetService('RunService').Stepped:connect(
	function()
		if ammo then
			if game.Workspace:FindFirstChild("Buy Ammo | $25") then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
					game.Workspace["Buy Ammo | $25"].Head.CFrame
			end
		end
	
	end
	)
	
	
	Input.InputBegan:Connect(
		function(hotkey)
			if hotkey.KeyCode == Enum.KeyCode[akey] then
				if Input:GetFocusedTextBox() == nil then
					local SavePos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
					ammo = true
				end
			end
		end
	)
	
	Input.InputEnded:Connect(
		function(hotkey)
			if hotkey.KeyCode == Enum.KeyCode[akey]then
				if Input:GetFocusedTextBox() == nil then
					ammo = false
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = SavePos
				end
			end
		end
	)
	
	local Input = game:GetService('UserInputService')
	local ammo = false
	
	game:GetService('RunService').Stepped:connect(
	function()
		if ammo then
			if game.Workspace:FindFirstChild("Glock | $200") then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
					game.Workspace["Glock | $200"].Head.CFrame
			end
		end
	
	end
	)
	
	
	Input.InputBegan:Connect(
		function(hotkey)
			if hotkey.KeyCode == Enum.KeyCode[gkey] then
				if Input:GetFocusedTextBox() == nil then
					local SavePos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
					ammo = true
				end
			end
		end
	)
	
	Input.InputEnded:Connect(
		function(hotkey)
			if hotkey.KeyCode == Enum.KeyCode[gkey]then
				if Input:GetFocusedTextBox() == nil then
					ammo = false
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = SavePos
				end
			end
		end
	)
	
	local Input = game:GetService('UserInputService')
	local ammo = false
	
	game:GetService('RunService').Stepped:connect(
	function()
		if ammo then
			if game.Workspace:FindFirstChild("Uzi | $150") then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
					game.Workspace["Uzi | $150"].Head.CFrame
			end
		end
	
	end
	)
	
	
	Input.InputBegan:Connect(
		function(hotkey)
			if hotkey.KeyCode == Enum.KeyCode[ukey] then
				if Input:GetFocusedTextBox() == nil then
					local SavePos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
					ammo = true
				end
			end
		end
	)
	
	Input.InputEnded:Connect(
		function(hotkey)
			if hotkey.KeyCode == Enum.KeyCode[ukey] then
				if Input:GetFocusedTextBox() == nil then
					ammo = false
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = SavePos
				end
			end
		end
	)
    

BindsSection:NewTextBox("Buy Uzi Key", "Changes your Buy Uzi Key", function(txt)
	ukey = txt
		cocka({
		Description = "Buy Uzi Key: " .. ukey ;
	Title = "big ballaz | v4.1";
	Duration = 5;
	});
end)

BindsSection:NewTextBox("Buy Ammo Key", "Changes your Buy Ammo Key", function(txt)
     akey = txt
     	cocka({
		Description = "Buy Ammo Key: " .. akey ;
	Title = "big ballaz | v4.1";
	Duration = 5;
	});
end)

