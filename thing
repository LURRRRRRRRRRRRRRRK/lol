local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local blue = Color3.fromRGB(9, 137, 207)
_G.ButtonColor = Color3.fromRGB(50,50,50)
_G.ButtonTextColor = Color3.fromRGB(255,255,255)
_G.ArrowColor = Color3.fromRGB(255,255,255)
_G.PointerColor =Color3.fromRGB(255,255,255)
_G.ToggleColor = Color3.fromRGB(255,255,255)
_G.SliderColor = Color3.fromRGB(255,255,255)
local CUSTOMIZATIONADADAD = library:CreateWindow("Customization")
local KEYBINDTHING = CUSTOMIZATIONADADAD:CreateFolder("Keybinds") 


KEYBINDTHING:Box("Buy Ammo","string",function(value)
   akey = value
    	cocka({
Description = "Buy Ammo Key: " .. tostring(akey);
Title = "big ballaz | v1";
Duration = 5;
});
end)

